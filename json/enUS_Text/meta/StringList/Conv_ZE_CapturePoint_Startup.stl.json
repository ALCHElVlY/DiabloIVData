 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "\"What is this device?\" ",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.1C5397",
   "szText": "Isn’t it beautiful? We call it the Basin. It’s built to carry the stuff of souls.",
   "hLabel": 2454686003
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.1C5397",
   "szText": "proud",
   "hLabel": 3800413440
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.1C5397",
   "szText": "Maybe you’ve seen the empty husks of flesh and bone wandering outside the Citadel. You can thank the khazra for that.",
   "hLabel": 1792226037
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line5.1C5397",
   "szText": "The Basin can pull free some of the souls still trapped inside. There’s not much that can be done for those folks now... but we can at least set their souls on their way.",
   "hLabel": 1129766071
  }
 ],
 "ptMapStringTable": "0"
}
