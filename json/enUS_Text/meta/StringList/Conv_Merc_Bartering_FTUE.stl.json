 "arStrings": [
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "MenuLabel",
   "szText": "\"Your daughter's here!\" ",
   "hLabel": 4215274549
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line1.189135",
   "szText": "Ah, so she is. It’s time she learned this part of the trade.",
   "hLabel": 2403832770
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir1.189135",
   "szText": "proud",
   "hLabel": 3749560207
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line11.189135",
   "szText": "The Pale Hand had a reputation. We never commanded the resources of the Wolves, so we had to make do. We’d fight someone else’s battles and take what their enemies left behind.",
   "hLabel": 1169095155
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line3.BAE4",
   "szText": "You were looters.",
   "hLabel": 3639732837
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir3.BAE4",
   "szText": "[with a wry smile]",
   "hLabel": 572571378
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line5.189135",
   "szText": "And I’m in good company, eh?",
   "hLabel": 1078912838
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir5.189135",
   "szText": "[with a comradely grin]",
   "hLabel": 2424640275
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line7.189135",
   "szText": "Earn a mercenary’s trust, make a trade agreement, and we’ll keep an eye out for whatever bits and bobs you might desire.",
   "hLabel": 416452872
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "Line9.189135",
   "szText": "Speak with Fay to get your contract sorted. You’ll pay in Pale Marks, coins daubed with chalk dust. Careful, friend. It shows you're one of us now.",
   "hLabel": 4048960202
  },
  {
   "__type__": "StringTableEntry",
   "__typeHash__": 3844612145,
   "szLabel": "VoiceDir9.189135",
   "szText": "winking with the last line",
   "hLabel": 1099720343
  }
 ],
 "ptMapStringTable": "0"
}
